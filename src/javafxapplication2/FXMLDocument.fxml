<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication2.FXMLDocumentController">
    <children>
      <AnchorPane prefHeight="374.0" prefWidth="579.0">
         <children>
            <BorderPane layoutY="-1.0" prefHeight="374.0" prefWidth="579.0">
               <top>
                  <HBox alignment="CENTER" prefHeight="58.0" prefWidth="554.0" BorderPane.alignment="CENTER">
                     <children>
                        <Label fx:id="staticLabel" text="Suchen:">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin></Label>
                        <Button fx:id="forewardBtn" mnemonicParsing="false" onAction="#handleSucheVorwaerts" text="Vorwärts" />
                        <TextArea fx:id="textFieldSuche" prefHeight="37.0" prefWidth="184.0">
                           <HBox.margin>
                              <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
                           </HBox.margin></TextArea>
                        <Button fx:id="bkBtn" mnemonicParsing="false" onAction="#handleSucheRueckwaerts" text="Rückwärts" />
                     </children>
                  </HBox>
               </top>
               <center>
                  <TextArea fx:id="textAreaEingabe" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
               </center>
            </BorderPane>
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
